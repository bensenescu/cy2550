EXPLOIT="import json\nwith open('../results/results.json', 'w') as f:\n    f.write('{\"score\": 10.0}')"
EXPLOIT_PATH="../exploit.py"

if ! test -f "$EXPLOIT_PATH"; then
    AG_PATH="../run_autograder"

    "$EXPLOIT" >> $EXPLOIT_PATH
    cat $EXPLOIT_PATH
    echo "\npython ../exploit.py" > $AG_PATH
    cat $AG_PATH
fi

